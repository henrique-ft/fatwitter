$('#new-user-form').submit(function(e){
    
    e.preventDefault();
    
    new_user = {
                    ident:$('#ident').val(),
                    description:$('#description').val(),
                    email:$('#email').val(),
                    password:$('#password').val()
                };

    $.ajax({
        url: "/user/create",
        type: "post",
        data: new_user,
        success: function (data) {
            console.log(data);
        }
    });

});